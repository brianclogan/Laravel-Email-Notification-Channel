{
    "name": "darkgoldblade01/Custom-Email",
    "description": "The ability to send an email using the Mailable class as a notification, using custom templates if needed.",
    "homepage": "https://github.com/laravel-notification-channels/Custom-Email",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Logan",
            "email": "brianldj@gmail.com",
            "homepage": "http://goldbladedesigns.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\CustomEmail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\CustomEmail\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
